    "nums = [1,2,3,1]\n",
    "from collections import Counter\n",
    "def duplicatenum(nums):\n",
    "    counter = Counter(nums)\n",
    "    for value in counter.values():\n",
    "        if value>1:\n",
    "            return True\n",
    "    return False\n"
   ]
